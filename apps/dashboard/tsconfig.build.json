{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "incremental": false,
    "tsBuildInfoFile": null,
    "skipLibCheck": true,
    "declaration": false,
    "declarationMap": false,
    "sourceMap": false
  },
  "exclude": [
    "node_modules", 
    "**/*.test.ts", 
    "**/*.test.tsx", 
    "**/*.spec.ts", 
    "**/*.spec.tsx",
    "src/**/*.d.ts"
  ]
}